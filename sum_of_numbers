//  28 -->  10 -->  1
//  16 -> 7 
//  99 -> 18 -> 9
//  991 -> 19 -> 10 -> 1 

#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    while(n>=10){
        int sum = 0;
        while (n > 0) {
            sum += n % 10;
            n /= 10;
    }
    n = sum;
    
}
cout<<n<<endl;
}
